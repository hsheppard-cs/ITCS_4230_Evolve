// Move left
if (keyboard_check(vk_left) {
		x += -4
    // flips image - Possibly delete?
		image_xscale = -1
} 

// Move right
if (keyboard_check(vk_right) {
		x += 4
		image_xscale = 1
}

// Jump
if (keyboard_check(vk_up))
{
  vspeed = -10	
} 

if ( keyboard_check(vk_space) )
{
	instance_create_layer(x, y, "Instances", obj_playerProjectile);
	
	// Need to still add conditional check - Communicate with Alarm
	// Alarm Code goes here...
}

